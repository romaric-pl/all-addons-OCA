"    <thead>\n"
"        <tr style=\"height: 15px; background-color: #d8d8d8;font-size: 13px;"
"font-weight:bold;\" align=\"center\" valign=\"middle\">\n"
"            <td>Data inizio</td>\n"
"            <td>Nome vento</td>\n"
"            <td>Data fine</td>\n"
"            <td>Stato</td>\n"
"        </tr>\n"
"    </thead>\n"
"    % for event in ctx[\"events\"]:\n"
"    <tr>\n"
"        <td><span>${event.date_begin}</span></td>\n"
"        <td><span>${event.name}</span></td>\n"
"        <td><span>${event.date_end}</td>\n"
"        <td><span style=\"font-weight:bold;\">${event.state}</td>\n"
"    </tr>\n"
"    % endfor\n"
"</table>\n"
"<pre>\n"
"--\n"
"Email automatica.\n"
"${object.company_id.name or ''}\n"
"</pre>\n"
"    "

