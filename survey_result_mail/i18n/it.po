#. module: survey_result_mail
#: model:mail.template,body_html:survey_result_mail.mail_template_user_input_result
msgid ""
"<div style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Dear ${object.partner_id.name or 'participant'}<br/><br/>\n"
"\n"
"        You'll find attached your survey answers so you can review them.\n"
"\n"
"        Thank you for your participation.\n"
"    </p>\n"
"</div>\n"
"    "
msgstr ""
"<div style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Steppabile ${object.partner_id.name or 'participant'}<br/><br/>\n"
"\n"
"        trova in allegato le sue risposte al sondaggio per poterle "
"revisionare.\n"
"\n"
"        La ringraziamo per la partecipazione.\n"
"    </p>\n"
"</div>\n"
"    "

