<?xml version="1.0" ?>
<odoo noupdate="1">
    <record id="fao_fishing_technique" model="product.attribute">
        <field name="name">FAO Fishing Technique</field>
        <field name="create_variant">no_variant</field>
        <field name="display_type">select</field>
    </record>
    <record
        id="fishing_technique_att_value_spearfishing"
        model="product.attribute.value"
    >
        <field name="name">Spearfishing</field>
        <field name="attribute_id" ref="fao_fishing_technique" />
    </record>
    <record id="fishing_technique_att_value_netting" model="product.attribute.value">
        <field name="name">Netting</field>
        <field name="attribute_id" ref="fao_fishing_technique" />
    </record>
    <record id="fishing_technique_att_value_trawl_nets" model="product.attribute.value">
        <field name="name">Trawl nets</field>
        <field name="attribute_id" ref="fao_fishing_technique" />
    </record>
    <record
        id="fishing_technique_att_value_enmanelle_nets"
        model="product.attribute.value"
    >
        <field name="name">Enmanelle nets and similar</field>
        <field name="attribute_id" ref="fao_fishing_technique" />
    </record>
    <record id="fishing_technique_att_value_purse_nets" model="product.attribute.value">
        <field name="name">Purse seines and nets hoisted</field>
        <field name="attribute_id" ref="fao_fishing_technique" />
    </record>
    <record id="fishing_technique_att_value_angling" model="product.attribute.value">
