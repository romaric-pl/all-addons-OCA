        <field name="attribute_id" ref="fao_fishing_technique" />
    </record>
    <record id="fishing_technique_att_value_trapping" model="product.attribute.value">
        <field name="name">Trapping</field>
        <field name="attribute_id" ref="fao_fishing_technique" />
    </record>
    <record
        id="fishing_technique_att_value_aquaculture"
        model="product.attribute.value"
    >
        <field name="name">Aquaculture</field>
        <field name="attribute_id" ref="fao_fishing_technique" />
    </record>
</odoo>
