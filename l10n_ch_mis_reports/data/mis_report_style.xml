<odoo noupdate="1">
    <record id="style_default" model="mis.report.style">
        <field name="name">CH default</field>
        <field name="dp_inherit">false</field>
        <field name="dp">2</field>
    </record>
    <record id="style_header" model="mis.report.style">
        <field name="name">CH header</field>
        <field name="font_size_inherit">false</field>
        <field name="font_size">large</field>
        <field name="font_weight_inherit">false</field>
        <field name="font_weight">bold</field>
    </record>
    <record id="style_header_indented" model="mis.report.style">
        <field name="name">CH header indented</field>
        <field name="font_weight_inherit">false</field>
        <field name="font_weight">bold</field>
        <field name="indent_level_inherit">false</field>
        <field name="indent_level">1</field>
    </record>
    <record id="style_details" model="mis.report.style">
        <field name="name">CH details</field>
        <field name="indent_level_inherit">false</field>
        <field name="indent_level">2</field>
    </record>
    <record id="style_details_indented" model="mis.report.style">
        <field name="name">CH details indented</field>
        <field name="indent_level_inherit">false</field>
        <field name="indent_level">3</field>
    </record>
</odoo>
